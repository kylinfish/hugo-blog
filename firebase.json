{
    "hosting": {
        "public": "public",
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ],

        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],

        "redirects": [
            {
                "source" : "/workshop3---my-virtual-host/",
                "destination" : "/sre-workshop3-virtualhost/",
                "type" : 301
            },
            {
                "source" : "/%E5%88%9D%E6%8E%A2-lumen-micro-framework---routing/",
                "destination" : "/lumen_route/",
                "type" : 301
            },
            {
                "source" : "/%E5%88%9D%E6%8E%A2-lumen-micro-framework---controller/",
                "destination" : "/lumen_controller/",
                "type" : 301
            },
            {
                "source" : "/%E5%9C%A8-terminal-%E7%92%B0%E5%A2%83%E4%B8%8B%E7%9A%84%E9%96%8B%E7%99%BC%E9%85%8D%E7%BD%AE/",
                "destination" : "/unix-like-develop-env/",
                "type" : 301
            },
            {
                "source" : "/%E8%B5%B0%E8%A8%AA-facebook-%E5%B7%A5%E7%A8%8B%E5%B8%AB%E7%9A%84%E8%81%B7%E6%B6%AF---%E5%BF%83%E5%BE%97%E5%88%86%E4%BA%AB/",
                "destination" : "/facebook-life-thinking/",
                "type" : 301
            },
            {
                "source" : "/%E6%B7%B1%E5%85%A5%E6%8E%A2%E8%A8%8E-json-web-token-jwt/",
                "destination" : "/jwt-advanced/",
                "type" : 301
            },
            {
                "source" : "/%E5%85%8D%E9%96%80%E6%AA%BB%E7%9A%84%E7%90%86%E8%B2%A1%E8%87%AA%E5%B7%B1%E7%9A%84%E8%B2%A1%E8%87%AA%E5%B7%B1%E7%90%86/",
                "destination" : "/finance_report_tool/",
                "type" : 301
            },
            {
                "source" : "/%E5%88%9D%E6%8E%A2-lumen-micro-framework---middleware/",
                "destination" : "/lumen_middleware/",
                "type" : 301
            },
            {
                "source" : "/workshop1---%E7%94%A8-gcp-%E8%87%AA%E6%9E%B6-web-server/",
                "destination" : "/sre-workshop1-gcp-vm-server/",
                "type" : 301
            },
            {
                "source" : "/%E5%9F%BA%E6%9C%AC%E7%9A%84%E7%B6%B2%E9%A0%81%E5%AE%89%E5%85%A8%E8%88%87%E9%98%B2%E8%AD%B7---sql-injection/",
                "destination" : "/sql_injection/",
                "type" : 301
            },
            {
                "source" : "/head-first-json-web-token-jwt/",
                "destination" : "/jwt-basic/",
                "type" : 301
            },
            {
                "source" : "phpmyadmin-%E8%88%87-json-%E7%9A%84%E5%B0%8F%E9%9B%B7%E8%B8%A9%E5%9D%91/",
                "destination" : "/phpmyadmin_with_json/",
                "type" : 301
            },
            {
                "source" : "workshop2---%E5%AE%89%E8%A3%9D-php-mysql-phpmyadmin/",
                "destination" : "/sre-workshop2-php-configure/",
                "type" : 301
            },
            {
                "source" : "/lumen-%E4%BD%BF%E7%94%A8-php-artisan-tinker/",
                "destination" : "/lumen_tinker/",
                "type" : 301
            },
            {
                "source" : "/%E8%AA%8D%E8%AD%98-google-pub%2fsub/",
                "destination" : "/pubsub/",
                "type" : 301
            }
        ],

        "headers": [
            {
                "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
                "headers": [
                    {
                        "key": "Access-Control-Allow-Origin",
                        "value": "*"
                    }
                ]
            },
            {
                "source": "**/*.@(jpg|jpeg|gif|png)",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "max-age=31536000"
                    }
                ]
            },
            {
                "source": "**/*.css)",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "max-age=259200"
                    }
                ]
            },
            {
                "source": "404.html",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "max-age=7200"
                    }
                ]
            }
        ],

        "cleanUrls": true
    }
}
